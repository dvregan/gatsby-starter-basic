{
  "name": "@dvregan/gatsby-starter-basic",
  "version": "1.0.0",
  "description": "A simple starting point for Gatsby sites.",
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "react",
    "react-dom"
  ],
  "author": "Damian Regan <polycode@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvregan/gatsby-starter-basic"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,html,md,mdx}",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "gatsby": "^2.20.12",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.3.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1"
  }
}
